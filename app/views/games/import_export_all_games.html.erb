<%= link_to 'Download Games JSON', export_all_games_path, class: 'btn btn-primary' %>

<%= form_with(url: import_all_games_path, method: :post, local: true, html: { multipart: true }) do |form| %>
  <div class="form-group">
    <%= form.label :file, "Choose JSON file to import" %>
    <%= form.file_field :file, accept: '.json', required: true %>
  </div>
  <%= form.submit "Import Games", class: 'btn btn-primary' %>
<% end %>
